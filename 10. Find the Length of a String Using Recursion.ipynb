   "source": [
    "def length_of_string(s):\n",
    "    if s == \"\":\n",
    "        return 0\n",
    "    return 1 + length_of_string(s[1:])\n",
    "\n",
    "# Example usage\n",
    "print(length_of_string(\"Hello\"))  # Output: 5\n"
   ]
