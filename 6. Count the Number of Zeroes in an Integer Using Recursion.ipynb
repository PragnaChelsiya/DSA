   "source": [
    "def count_zeroes(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    if n < 10:\n",
    "        return 0\n",
    "    return (1 if n % 10 == 0 else 0) + count_zeroes(n // 10)\n",
    "\n",
    "# Example usage\n",
    "print(count_zeroes(10503))  # Output: 2\n"
   ]
