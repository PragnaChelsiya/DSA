   "source": [
    "def sum_natural_numbers(n):\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    return n + sum_natural_numbers(n - 1)\n",
    "\n",
    "# Example usage\n",
    "print(sum_natural_numbers(5))  # Output: 15\n"
   ]
