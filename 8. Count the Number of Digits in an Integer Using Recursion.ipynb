    "def count_digits(n):\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    return 1 + count_digits(n // 10)\n",
    "\n",
    "# Example usage\n",
    "print(count_digits(12345))  # Output: 5\n"
   ]
