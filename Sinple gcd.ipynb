    "\n",
    "def simple_gcd(m,n):\n",
    " fm=[]\n",
    " for i in range(1,m+1):\n",
    "   if (m%i)==0:\n",
    "      fm.append(i)\n",
    "\n",
    " fn=[]\n",
    " for j in range(1,n+1):\n",
    "   if (n%j)==0:\n",
    "      fn.append(j)\n",
    "\n",
    " cf=[]\n",
    " for k in fm:\n",
    "  if k in fn:\n",
    "      cf.append(k)\n",
    "\n",
    " return(cf[-1])\n",
    "print(simple_gcd(14,63))"
   ]
