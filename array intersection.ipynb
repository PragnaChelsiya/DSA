    "def intersection(arr1, arr2):\n",
    "    return list(set(arr1) & set(arr2))\n",
    "\n",
    "# Example usage:\n",
    "arr1 = [1, 2, 2, 3, 4]\n",
    "arr2 = [2, 3, 3, 5]\n",
    "result = intersection(arr1, arr2)\n",
    "print(result)\n"
