   "source": [
    "import numpy as np\n",
    "x=np.array([1,2,3,4,5])\n",
    "def prod_ExceptSel(x):\n",
    "   n=len(x)\n",
    "   res=[1]*n\n",
    "   for i in range(n):\n",
    "     for j in range(n):\n",
    "         if i !=j:\n",
    "            res[i]*=x[j]\n",
    "   return res\n",
    "print(\"Original array:\",x)\n",
    "result=prod_ExceptSel(x)\n",
    "print(\"Product Array:\",result)"
   ]
